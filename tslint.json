{
	"extends": "tslint:recommended",
	"rules": {
		"indent": "tabs",
		"quotemark": "single",
		"trailing-comma": "never",
		"no-var-requires": false,
		"variable-name": {
			"check-format": {
				"allow-leading-underscore": true
			}
		},
		"object-literal-sort-keys": false,
		"no-console": [
			true,
			"debug",
			"info",
			"log",
			"time",
			"timeEnd",
			"trace",
			"error"
		]
	}
}